if type brew &>/dev/null; then
  FPATH=$(brew --prefix)/share/zsh/zsh-completions:$FPATH

  autoload -Uz compinit
  compinit
fi

export CLICOLOR=1
export PATH=/opt/homebrew/opt/node@16/bin:/usr/local/bin:/usr/local/sbin:$PATH:$HOME/Scripts

# Let's get more history going.
HISTSIZE=10000
HISTFILESIZE=10000
SAVEHIST=$HISTSIZE
alias history="history 1"
